<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Numbers</title>
</head>
<body>
@model int[]
<h2>Here are some numbers!</h2>
@foreach (var number in Model)
{
    <p>@number</p>
}
    
</body>
</html>